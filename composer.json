{
  "name": "in2it/phpunit-docker",
  "description": "Example to run phpunit in parallel with Docker",
  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "fzaninotto/faker": "^1.6",
    "squizlabs/php_codesniffer": "^2.7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Michelangelo van Dam",
      "email": "michelangelo@in2it.be"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">= 7.0"
  },
  "autoload": {
    "psr-4": {
      "Profman\\": "src/Profman"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Profman\\Test\\": "tests/Profman"
    }
  }
}
